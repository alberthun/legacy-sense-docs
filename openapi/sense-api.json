    "/v2/login": {
      "post": {
        "summary": "Authenticate users",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["email"],
                "properties": {
                  "email": {
                    "example": "jolly@poly.com",
                    "type": "string"
                  },
                  "password": {
                    "example": "secret password",
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "tags": ["Authenticate"],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string",
                      "description": "jwt token"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
