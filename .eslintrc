{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "graphql": false
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "eol-last": 0,
    "arrow-parens": 0,
    "quotes": 0,
    "object-curly-newline": 0,
    "function-paren-newline": 0,
    "arrow-body-style": 0,
    "no-prototype-builtins": 0
  }
}
