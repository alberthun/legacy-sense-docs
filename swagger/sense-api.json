        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Authenticate"
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
